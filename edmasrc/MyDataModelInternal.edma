DataModel MyDataModel
{
    Kind Person
    {
        firstName+ : Name,
        lastName+ : Name,
        personalMail+ : Email,
        Unique(personalMail)
    }

    Kind Teacher extends Person
    {
        schoolMail+ : Email,
        Unique(schoolMail)
    }

    Kind Course
    {
        name+ : Name,
        Unique(name)
    }

    
}